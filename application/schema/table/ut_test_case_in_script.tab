create table UT_TEST_CASE_IN_SCRIPT
(
  id              NUMBER not null,
  test_script_id  NUMBER not null,
  test_case_id    NUMBER not null,
  script_order    NUMBER not null,
  catch_exception CHAR(1),
  expected_result VARCHAR2(10)
)
;
comment on column UT_TEST_CASE_IN_SCRIPT.id
  is 'Primarni klic zaznamu';
comment on column UT_TEST_CASE_IN_SCRIPT.test_script_id
  is 'Cizi klic do tabulky UT_TEST_SCRIPT, testovaci scenar';
comment on column UT_TEST_CASE_IN_SCRIPT.test_case_id
  is 'Cizi klic do tabulky UT_TEST_CASE, evidence test case';
comment on column UT_TEST_CASE_IN_SCRIPT.script_order
  is 'Urcuje poradi spusteni jednotlivych test case v ramci testovaciho scenare';
comment on column UT_TEST_CASE_IN_SCRIPT.catch_exception
  is '(''Y'',''N'') Urcuje zdali pri chybe test case ma odchytnout a potlacit vyjimku a pokracovat dal v behu testovaciho scenare';
comment on column UT_TEST_CASE_IN_SCRIPT.expected_result
  is 'Typ ocekavaneho vysledku scenare (''OK'',''RAISE'')';
alter table UT_TEST_CASE_IN_SCRIPT
  add constraint UT_TEST_CASE_IN_SCRIPT_PK primary key (ID);
alter table UT_TEST_CASE_IN_SCRIPT
  add constraint UT_TEST_SCRIPT_CASE_CAS_FK foreign key (TEST_CASE_ID)
  references UT_TEST_CASE (ID);
alter table UT_TEST_CASE_IN_SCRIPT
  add constraint UT_TEST_SCRIPT_CASE_SCR_FK foreign key (TEST_SCRIPT_ID)
  references UT_TEST_SCRIPT (ID);

